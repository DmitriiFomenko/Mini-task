<Window x:Class=".Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800" Closing="Window_Closing">
    <Grid>
        <CheckBox x:Name="checkBox1"  Content="1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Tag="1" Click="checkBox1_Click"/>
        <CheckBox x:Name="checkBox2" Content="2" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Click="checkBox1_Click" Tag="2"/>
        <CheckBox x:Name="checkBox3" Content="3" HorizontalAlignment="Left" Margin="10,50,0,0" VerticalAlignment="Top" Click="checkBox1_Click" Tag="3"/>
        <CheckBox x:Name="checkBox4" Content="4" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Click="checkBox1_Click" Tag="4"/>
        <CheckBox x:Name="checkBox5" Content="5" HorizontalAlignment="Left" Margin="10,90,0,0" VerticalAlignment="Top" Click="checkBox1_Click" Tag="5"/>
        <CheckBox x:Name="checkBox6" Content="6" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Click="checkBox1_Click" Tag="6"/>

    </Grid>
</Window>
